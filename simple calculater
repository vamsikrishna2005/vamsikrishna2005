class caliculater:
    def __init__(self,n1,n2) -> None:
         self.n1 = n1
         self.n2 = n2
    def add(self):
         return self.n1 + self.n2     
    def sub(self):
         return self.n1 - self.n2
    def muilt(self):
         return self.n1 * self.n2
    def divison(self):
         return self.n1 / self.n2
    
c = caliculater(10,5)
sum1 = c.add()
print(sum1)
sub1 = c.sub()
print(sub1)
mult1 = c.muilt()
print(mult1)
div = c.divison()
print(div)
